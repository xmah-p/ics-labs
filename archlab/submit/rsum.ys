rsum_list: 
    xorq %rax, %rax
    andq %rdi, %rdi
    je done
    mrmovq (%rdi), %rdx
    pushq  %rdx          # long val = ls->val;
    mrmovq 8(%rdi), %rdi
    call rsum_list       # long rest = rsum_list(ls->next)
    popq  %rdx
    addq %rax, %rdx
    rrmovq %rdx, %rax    # return val + rest
done:
    ret

main:	
    irmovq ele1, %rdi
	call rsum_list    # rsum_list(ele1)
	ret

    .pos 0x200
stack:
